   "source": [
    "import itertools\n",
    "print(\"The truth table for AND gate is: \")\n",
    "print(\"A\\tB\\tResult\")\n",
    "\n",
    "intputCombination = list(itertools.product([True, False],repeat=2))\n",
    "\n",
    "for combiantion in intputCombination:\n",
    "    A,B = combiantion\n",
    "    result = (A and B)\n",
    "    print(f\"{int(A)}\\t{int(B)}\\t{int(result)}\")\n"
   ]
