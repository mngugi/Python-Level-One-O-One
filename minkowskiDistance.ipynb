    "import numpy as np\n",
    "\n",
    "def minkowski_distance(x, y, p):\n",
    "    return np.sum(np.abs(x-y)**p) ** (1/p)\n",
    "\n",
    "\n",
    "x = np.array([10,25,36])\n",
    "y = np.array([40,55,66])\n",
    "\n",
    "distance = minkowski_distance(x, y, p=5)\n",
    "print(\"Minkowski Distance is: \", distance)"
